begin
  # Seven Nation Army by The White Stripes
  
  use_bpm 120
  use_synth :fm
  use_synth_defaults sustain: 0.25
  
  
  notes = [:e3,:e3,:g3,:e3,:r,:d3,:c3,:b2 ]
  i = 0
  
  live_loop :white_stripes do
    9.times do
      play(notes[i])
      sleep 0.5
      i = i + 1
      print(i)
    end
    i = 0
  end
  
end
